
  /* 骨架屏 */
.skeleton-view{
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    background-color: #FFFFFF;
    z-index: 999;
    overflow: hidden;
}
.skeleton-global{
    background: #f3f3f3;
    border-radius: 8rpx;
    animation: blink 1.2s ease-in-out infinite;
}
@keyframes blink{
50%{
        opacity: 0.5;
}
}
